{
    "author": "Tumist2676",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "description": "This extension adds a button to context menu, which allows to search selected text with Google",
    "icons": {
        "16": "icons/16px.png",
        "20": "icons/20px.png",
        "25": "icons/25px.png",
        "30": "icons/30px.png",
        "32": "icons/32px.png",
        "40": "icons/40px.png",
        "48": "icons/48px.png",
        "64": "icons/64px.png"
    },
    "name": "Google Context Search",
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "version": "0.1",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}
